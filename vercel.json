{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/proxy"
        }
    ],   
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=60, s-maxage=120"
                }
            ]
        }
    ],
    "regions": ["iad1", "sfo1", "lhr1"]
}
